content = "（不是今天早上才碰见过的吗……）"
metadata/_custom_type_script = "uid://bs6xd4aigyj4"

[sub_resource type="Resource" id="Resource_eo8wo"]
script = ExtResource("1_b6ntj")
character_name = "咖啡屋老板："
content = "那是不满的眼神！"
metadata/_custom_type_script = "uid://bs6xd4aigyj4"

[sub_resource type="Resource" id="Resource_0fsmj"]
script = ExtResource("1_b6ntj")
character_name = "咖啡屋老板："
content = "寻常的人，只会把咖啡当作饮品；而像你我这样的人，则会在恰当的时机，将承载自己苦涩的黑咖啡泼洒在对面之人的头顶。"
metadata/_custom_type_script = "uid://bs6xd4aigyj4"

[sub_resource type="Resource" id="Resource_d0l6v"]
script = ExtResource("1_b6ntj")
character_name = "咖啡屋老板："
content = "你既然有一颗不安于现状的心，就不该像我一样，拘泥在这个小小的乐园中。"
metadata/_custom_type_script = "uid://bs6xd4aigyj4"

[sub_resource type="Resource" id="Resource_3p5ns"]
