script = ExtResource("1_b6ntj")
character_name = "咖啡屋老板："
content = "黑咖啡的苦涩，不及我人生的苦涩的十分之一。"
metadata/_custom_type_script = "uid://bs6xd4aigyj4"

[sub_resource type="Resource" id="Resource_4nmsj"]
script = ExtResource("1_b6ntj")
character_name = "咖啡屋老板："
content = "好久不见了，小帅哥。即使已经过了这么久，我还是能看出来，你有着和我相似的眼神。"
metadata/_custom_type_script = "uid://bs6xd4aigyj4"

[sub_resource type="Resource" id="Resource_wj0up"]
script = ExtResource("1_b6ntj")
